{
    "name": "reedware/nova-field-manager",
    "description": "Creates a field facade for referencing fields.",
    "keywords": ["laravel", "nova", "fields"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\NovaFieldManager\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Reedware\\NovaFieldManager\\NovaFieldManagerServiceProvider"
            ],
            "aliases": {
                "Field": "Reedware\\NovaFieldManager\\Facade"
            }
        }
    }
}
